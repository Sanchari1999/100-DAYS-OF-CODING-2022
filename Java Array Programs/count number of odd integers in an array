import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int array[]= new int[10];
		int count=0;
		
		System.out.println("Enter 10 array elements:");
		
		for(int i=0; i<10; i++){
		    array[i]=sc.nextInt();
		}
		
		for(int j=0; j<10; j++){
		    if(array[j]%2!=0){
		        count++;
		    }
		}
		System.out.println("The no of odd elements are " + count);
	}
}
