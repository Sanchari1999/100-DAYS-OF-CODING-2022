import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int array[]= new int[10];
		
		System.out.println("Enter 10 array elements:");
		
		for(int i=0; i<10; i++){
		    array[i]=sc.nextInt();
		}
		
		System.out.println("The array elements are:");
		for(int j=0; j<10; j++){
		    System.out.println(array[j]);
		}
	}
}
